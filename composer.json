{
  "require": {
    "craftcms/cms": "^3.6.4",
    "craftcms/commerce": "3.2.17.3",
    "craftcms/redactor": "2.8.5",
    "dukt/videos": "2.0.13",
    "solspace/craft-freeform": "3.10.11",
    "twentyfourhoursmedia/comments-work": "1.3.0",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
